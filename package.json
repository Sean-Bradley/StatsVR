    "@types/node": "^18.11.9",
    "@types/three": "^0.146.0",
    "html-webpack-plugin": "^5.5.0",
    "three": "^0.146.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
