    "@types/node": "^13.13.15",
    "@types/three": "^0.128.0",
    "html-webpack-plugin": "^5.3.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
